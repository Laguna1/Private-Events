<h1 class='text-center'>Log in</h1>

<div class="row">
  <div class="col-md-6 offset-3">
     <%= form_tag '/login' do %>
   <%= label_tag :username%>
   <%= text_field_tag :username %>
   <%= label_tag :password%>
   <%= password_field_tag :password%>
   <%= submit_tag "Login"%>
<%end%>

  </div>
</div>